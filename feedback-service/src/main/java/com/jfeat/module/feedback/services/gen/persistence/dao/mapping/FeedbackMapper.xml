<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.module.feedback.services.gen.persistence.dao.FeedbackMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jfeat.module.feedback.services.gen.persistence.model.Feedback">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="user_name" property="userName" />
        <result column="contact" property="contact" />
        <result column="feedback" property="feedback" />
        <result column="feedback_image" property="feedbackImage" />
        <result column="status" property="status" />
        <result column="solved_by" property="solvedBy" />
        <result column="solved_note" property="solvedNote" />
        <result column="solved_user_id" property="solvedUserId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, user_name, contact, feedback, feedback_image, status, solved_by, solved_note, solved_user_id, create_time, update_time
    </sql>

</mapper>
