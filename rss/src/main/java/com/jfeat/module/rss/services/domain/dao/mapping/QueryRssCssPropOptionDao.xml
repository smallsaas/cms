<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.module.rss.services.domain.dao.QueryRssCssPropOptionDao">
                                                                                                                                                                                                <sql id="Base_Column_List">
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        t_rss_css_prop_option.id, t_rss_css_prop_option.css_name_props_id AS cssNamePropsId, t_rss_css_prop_option.prop_name AS propName, t_rss_css_prop_option.prop_option AS propOption, t_rss_css_prop_option.note
    </sql>

    
    
    <select id="queryMasterModel" resultType="RssCssPropOptionModel">
       SELECT t_rss_css_prop_option.*
       FROM t_rss_css_prop_option
       WHERE t_rss_css_prop_option.id=#{id}
       GROUP BY t_rss_css_prop_option.id
    </select>



                    
    <select id="findRssCssPropOptionPage" resultType="RssCssPropOptionRecord" parameterType="RssCssPropOptionRecord">
            SELECT <include refid="Base_Column_List"/>

        

        FROM t_rss_css_prop_option

        
        
        WHERE 1=1
        

        
                                                                            <if test="record.id != null and record.id>0 ">
                                    AND t_rss_css_prop_option.id LIKE CONCAT('%',#{record.id},'%')
                            </if>
                                                                    

                                                                                                                        <if test="record.cssNamePropsId != null and record.cssNamePropsId>0 ">
                                    AND t_rss_css_prop_option.css_name_props_id LIKE CONCAT('%',#{record.cssNamePropsId},'%')
                            </if>
                                                                    

                                                                                        <if test="record.propName != null and record.propName!= ''">
                                    AND t_rss_css_prop_option.prop_name LIKE CONCAT('%',#{record.propName},'%')
                            </if>
                                                                    

                                                                                        <if test="record.propOption != null and record.propOption!= ''">
                                    AND t_rss_css_prop_option.prop_option LIKE CONCAT('%',#{record.propOption},'%')
                            </if>
                                                                    <if test="record.note != null and record.note!= ''">
                                    AND t_rss_css_prop_option.note LIKE CONCAT('%',#{record.note},'%')
                            </if>
        
        <if test="startTime != null">
            <![CDATA[AND t_rss_css_prop_option.start_time >= DATE(#{startTime})]]>
        </if>
        <if test="endTime != null">
            <![CDATA[AND t_rss_css_prop_option.end_time <= DATE(#{endTime}]]>
        </if>
            <!--
        <if test="search != null and search != ''">
            OR t_rss_css_prop_option.name LIKE CONCAT('%',#{search},'%')
        </if>
        -->
                                    </select>
</mapper>